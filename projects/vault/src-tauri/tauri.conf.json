{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "keepkey-gui",
  "version": "0.1.1",
  "identifier": "com.keepkey-gui.app",
  "build": {
    "devUrl": "http://localhost:1430",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "keepkey-desktop",
        "width": 1400,
        "height": 920
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
